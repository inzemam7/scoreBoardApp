{
  "expo": {
    "name": "scoreBoardApp",
    "slug": "scoreBoardApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/iconimg.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconimg.png",//change this later
        "backgroundColor": "#ffffff"//this as well
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          // "image": "./assets/images/spalsh.mp4",//splash change as 
          "adaptiveIcon":"./assets/images/spalsh.mp4",//splash change as
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "golden"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
